{
  "name": "github-source-public",
  "config": {
    "connector.class": "com.nhswd.kafka.custom.http.connector.HttpSourceConnector",
    "http.headers": "Content-Type=application/json",
    "tasks.max": "1",
    "http.url": "${GITHUB_URL}",
    "http.event.types": "${GITHUB_EVENT_TYPES}",
    "http.poll.interval.ms": "${KAFKA_CONNECT_POLL_INTERVAL_MS}",
    "http.connect.timeout.ms": "${KAFKA_CONNECT_HTTP_TIMEOUT_MS}",
    "http.read.timeout.ms": "${KAFKA_CONNECT_HTTP_READ_TIMEOUT_MS}",
    "topic": "${KAFKA_TOPIC}",
    "http.method": "${KAFKA_CONNECT_HTTP_METHOD}",
    "http.auth.bearer": "${GITHUB_AUTH_TOKEN_CLASSIC}",
    "value.converter": "org.apache.kafka.connect.storage.StringConverter",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter"
  }
}
